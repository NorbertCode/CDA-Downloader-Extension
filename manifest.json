{
  "manifest_version": 3,
  "name": "CDA Downloader",
  "description": "Download videos from cda.pl",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.cda.pl/video/*"],
      "js": ["siteHandler.js"]
    }
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "downloads",
    "storage"
  ]
}
